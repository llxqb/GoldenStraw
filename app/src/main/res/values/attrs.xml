<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="HorizontalListView">
        <attr name="dividerWidth" format="dimension" />
        <attr name="android:divider" />
        <attr name="android:requiresFadingEdge" />
        <attr name="android:fadingEdgeLength" />
    </declare-styleable>
    <declare-styleable name="RoundAngleImageView">
        <attr name="roundWidth" format="dimension" />
        <attr name="roundHeight" format="dimension" />
    </declare-styleable>
    <declare-styleable name="circleimageview">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_inside_color" format="color" />
        <attr name="border_outside_color" format="color" />
    </declare-styleable>

    <!--ListItem1的自定义属性-->
    <declare-styleable name="ListItem1">
        <attr name="itemIcon" format="reference" />
        <attr name="itemTitle" format="reference" />
        <attr name="itemCenter" format="boolean" />
        <attr name="itemArrow" format="boolean" />
    </declare-styleable>

    <!--UserItem1的自定义属性-->
    <declare-styleable name="UserItem1">
        <attr name="userTitle" format="reference" />
        <attr name="userContent" format="reference" />
        <attr name="userCenter" format="boolean" />
        <attr name="userArrow" format="boolean" />
    </declare-styleable>

    <!--带白边框的圆形图片 -->
    <declare-styleable name="CircularImage">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>

    <!--省市级联 -->
    <declare-styleable name="wheelview">
        <attr name="gravity">
            <enum name="center" value="17" />
            <enum name="left" value="3" />
            <enum name="right" value="5" />
        </attr>
        <attr name="textColorOut" format="color" />
        <attr name="textColorCenter" format="color" />
        <attr name="dividerColor" format="color" />
    </declare-styleable>

    <!-- 水波圆 -->
    <declare-styleable name="myCircle">
        <attr name="roundColor" format="color"></attr>
        <attr name="roundWidths" format="dimension"></attr>
        <attr name="textSize" format="dimension"></attr>
        <attr name="textColor" format="color"></attr>
        <attr name="arcColor" format="color"></attr>
        <attr name="text" format="string"></attr>
    </declare-styleable>
</resources>
